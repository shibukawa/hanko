{
  "name": "@teamhanko/hanko-client",
  "version": "0.0.1-alpha.8",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "source": "src/Client.ts",
  "exports": {
    "require": "./dist/client.cjs",
    "default": "./dist/client.modern.js"
  },
  "main": "./dist/client.cjs",
  "module": "./dist/client.module.js",
  "unpkg": "./dist/client.umd.js",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "format": "pretty-quick --staged",
    "build": "microbundle",
    "dev": "microbundle watch",
    "docs": "jsdoc -r -c jsdoc.json -d docs -P package.json --verbose --debug"
  },
  "description": "This package provides basic functionality for the UI integration with the hanko backend. It is meant for use in browsers only.",
  "repository": "github:teamhanko/hanko",
  "author": "Hanko GmbH <developers@hanko.io>",
  "license": "MIT",
  "keywords": [
    "hanko",
    "passkey",
    "webauthn",
    "passcode",
    "password",
    "login",
    "register"
  ],
  "homepage": "https://hanko.io",
  "devDependencies": {
    "@types/preact-custom-element": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "better-docs": "^2.7.2",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "microbundle": "^0.15.0",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@github/webauthn-json": "^2.0.0",
    "@types/js-cookie": "^3.0.2",
    "js-cookie": "^3.0.1"
  }
}
