{
  "opts": {
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["optional"],
    "dictionaries": ["jsdoc", "closure"]
  },
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.(ts)$"
  },
  "templates": {
    "cleverLinks": true
  },
  "sourceType": "module"
}
